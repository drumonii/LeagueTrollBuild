    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../../static/semantic-ui/dist",
      "uncompressed": "../../static/semantic-ui/components/",
