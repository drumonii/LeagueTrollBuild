/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/troll-build-app",
    "types": [],
    "baseUrl": "src",
    "paths": {
      "@ltb-layout/*": [
        "./app/layout/*"
      ],
      "@ltb-directive/*": [
        "./app/directive/*"
      ],
      "@ltb-guard/*": [
        "./app/guard/*"
      ],
      "@ltb-interceptor/*": [
        "./app/interceptor/*"
      ],
      "@ltb-model/*": [
        "./app/model/*"
      ],
      "@ltb-service/*": [
        "./app/service/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
