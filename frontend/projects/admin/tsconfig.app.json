/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/admin-app",
    "types": [],
    "baseUrl": "src",
    "paths": {
      "@admin-layout/*": [
        "./app/layout/*"
      ],
      "@admin-interceptor/*": [
        "./app/interceptor/*"
      ],
      "@admin-service/*": [
        "./app/service/*"
      ],
      "@admin-guard/*": [
        "./app/guard/*"
      ],
      "@admin-security/*": [
        "./app/security/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
