{
  "name": "league-troll-build",
  "version": "2.5.17-SNAPSHOT",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve troll-build",
    "start-admin": "ng serve admin",
    "build": "ng build troll-build --prod",
    "build-admin": "ng build admin --prod",
    "build-all": "concurrently \"npm run build\" \"npm run build-admin\"",
    "build-with-stats": "ng build troll-build --prod --statsJson=true",
    "test": "ng test troll-build --code-coverage",
    "test-admin": "ng test admin --code-coverage",
    "test-all": "concurrently \"npm run test\" \"npm run test-admin\"",
    "test-ci": "ng test troll-build --browsers ChromeHeadless --code-coverage --watch=false",
    "test-admin-ci": "ng test admin --browsers ChromeHeadless --code-coverage --watch=false",
    "test-all-ci": "concurrently \"npm run test-ci\" \"npm run test-admin-ci\"",
    "lint": "ng lint",
    "e2e": "ng e2e troll-build --webdriverUpdate=false",
    "e2e-admin": "ng e2e admin --webdriverUpdate=false",
    "e2e-all": "concurrently \"npm run e2e\" \"npm run e2e-admin\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@clr/angular": "^2.3.3",
    "@clr/icons": "^2.3.3",
    "@clr/ui": "^2.3.3",
    "@nebular/eva-icons": "^4.6.0",
    "@nebular/theme": "^4.6.0",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@webcomponents/custom-elements": "^1.3.1",
    "core-js": "^2.6.5",
    "eva-icons": "^1.1.2",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "~3.4.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.12.8",
    "codelyzer": "^5.1.2",
    "concurrently": "^4.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.8.0",
    "typescript": "3.5.3"
  }
}
