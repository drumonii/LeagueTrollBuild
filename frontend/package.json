{
  "name": "league-troll-build",
  "version": "2.5.15-SNAPSHOT",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve troll-build",
    "start-admin": "ng serve admin",
    "build": "ng build troll-build --prod",
    "build-admin": "ng build admin --prod",
    "build-all": "concurrently \"npm run build\" \"npm run build-admin\"",
    "build-with-stats": "ng build troll-build --prod --statsJson=true",
    "analyze-stats": "webpack-bundle-analyzer dist/troll-build/stats-es2015.json",
    "test": "ng test troll-build --code-coverage",
    "test-admin": "ng test admin --code-coverage",
    "test-all": "concurrently \"npm run test\" \"npm run test-admin\"",
    "test-ci": "ng test troll-build --browsers ChromeHeadless --code-coverage --watch=false",
    "test-admin-ci": "ng test admin --browsers ChromeHeadless --code-coverage --watch=false",
    "test-all-ci": "concurrently \"npm run test-ci\" \"npm run test-admin-ci\"",
    "lint": "ng lint",
    "e2e": "ng e2e troll-build --webdriverUpdate=false",
    "e2e-admin": "ng e2e admin --webdriverUpdate=false",
    "e2e-all": "concurrently \"npm run e2e\" \"npm run e2e-admin\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.2",
    "@angular/common": "8.2.2",
    "@angular/compiler": "8.2.2",
    "@angular/core": "8.2.2",
    "@angular/forms": "8.2.2",
    "@angular/platform-browser": "8.2.2",
    "@angular/platform-browser-dynamic": "8.2.2",
    "@angular/router": "8.2.2",
    "@clr/angular": "^2.1.1",
    "@clr/icons": "^2.1.1",
    "@clr/ui": "^2.1.1",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@webcomponents/custom-elements": "^1.2.4",
    "bulma": "^0.7.5",
    "core-js": "^2.6.5",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular/cli": "8.2.2",
    "@angular/compiler-cli": "8.2.2",
    "@angular/language-service": "8.2.2",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.0.12",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.3",
    "tslint": "~5.15.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
