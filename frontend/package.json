{
  "name": "league-troll-build",
  "version": "2.5.22-SNAPSHOT",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve troll-build",
    "start-admin": "ng serve admin",
    "build": "ng build troll-build --prod",
    "build-admin": "ng build admin --prod",
    "build-with-stats": "ng build troll-build --prod --statsJson=true",
    "test": "ng test troll-build",
    "test-admin": "ng test admin",
    "test-all": "concurrently \"npm run test\" \"npm run test-admin\"",
    "test-ci": "ng test troll-build --browsers ChromeHeadless --code-coverage --watch=false",
    "test-admin-ci": "ng test admin --browsers ChromeHeadless --code-coverage --watch=false",
    "test-all-ci": "concurrently \"npm run test-ci\" \"npm run test-admin-ci\"",
    "lint": "ng lint",
    "e2e": "ng e2e troll-build --webdriverUpdate=false",
    "e2e-admin": "ng e2e admin --webdriverUpdate=false",
    "e2e-all": "concurrently \"npm run e2e\" \"npm run e2e-admin\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/forms": "9.1.11",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/router": "9.1.11",
    "@clr/angular": "^3.0.1",
    "@clr/core": "^3.0.1",
    "@clr/icons": "^3.0.1",
    "@clr/ui": "^3.0.1",
    "@nebular/eva-icons": "^5.0.0",
    "@nebular/theme": "^5.0.0",
    "@ngx-loading-bar/core": "^5.0.0",
    "@ngx-loading-bar/http-client": "^5.0.0",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "core-js": "^2.6.5",
    "eva-icons": "^1.1.3",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "9.1.11",
    "@angular/language-service": "9.1.11",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~5.4.3",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.1",
    "typescript": "3.7.5"
  }
}
